{
  "name": "app",
  "version": "1.2.0",
  "description": "React Boilerplate with support for Redux, TypeScript, Hot Reloading, Routing, Isomorphic rendering, TSLint, SASS, Testing",
  "main": "index.tsx",
  "repository": "https://github.com/kiresuah/ReactTypeScript",
  "scripts": {
    "new": "ts-node ./config/new_component.ts",
    "dev": "ts-node ./config/server.ts",
    "tdd": "npm run test -- --watch",
    "test": "mocha -R min --compilers ts:ts-node/register,tsx:ts-node/register --require ./__tests__/helper.tsx --recursive ./__tests__",
    "cover": "istanbul cover -x **/__tests__/** -e .tsx node_modules/mocha/bin/_mocha -- --compilers ts:ts-node/register,tsx:ts-node/register --require ./__tests__/helper.tsx --recursive ./__tests__",
    "cover:ci": "istanbul cover -x **/__tests__/** -e .tsx node_modules/mocha/bin/_mocha --reporter lcovonly -- --compilers ts:ts-node/register,tsx:ts-node/register --require ./__tests__/helper.tsx --recursive ./__tests__",
    "lint": "tslint -c tslint.json 'src/**/*.ts*' --fix",
    "build": "webpack --config ./config/webpack.production.config.ts --progress --profile --colors",
    "start": "nodemon ./config/server.production.tsx --exec ts-node"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/compression": "^0.0.33",
    "@types/enzyme": "^2.7.1",
    "@types/express": "^4.0.35",
    "@types/jsdom": "^2.0.29",
    "@types/mocha": "^2.2.38",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.0",
    "@types/react": "^15.0.1",
    "@types/react-dom": "^0.14.20",
    "@types/react-hot-loader": "^3.0.0",
    "@types/react-motion": "^0.0.21",
    "@types/react-redux": "^4.4.35",
    "@types/redux": "^3.6.0",
    "@types/redux-promise": "^0.5.24",
    "@types/redux-thunk": "^2.1.0",
    "@types/sinon": "^1.16.34",
    "@types/webpack": "^2.2.2",
    "@types/webpack-dev-server": "^1.12.6",
    "@types/webpack-env": "^1.13.0",
    "autoprefixer": "^6.6.1",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "css-loader": "^0.27.2",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.7.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "v3.0.0-beta.6",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-react": "^2.3.0",
    "typescript-require": "^0.2.9-1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "@types/marked": "^0.0.28",
    "axios": "^0.15.3",
    "compression": "^1.6.2",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "history": "4.5.1",
    "js-cookie": "^2.1.3",
    "marked": "^0.3.6",
    "morgan": "^1.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "5.0.0-alpha.3",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3"
  }
}
